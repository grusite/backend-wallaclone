{
  "name": "backend-wallaclone",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env MONGO_URI=${MONGO_URI} node ./bin/www",
    "dev": "cross-env NODE_ENV=development DEBUG=wallaclone:* nodemon",
    "installDB": "cross-env DEBUG=wallaclone:* node ./lib/installDB.js",
    "test": "jest",
    "test:dev": "jest --watch --verbose",
    "pretty": "prettier --write '**/*.{js,json}'",
    "lint": "eslint test routes services app.js",
    "precommit": "npm ls >/dev/null && lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix --max-warnings 0 --ignore-pattern '!.eslintrc.js'",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "debug": "^2.6.9",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "express": "~4.16.1",
    "express-deliver": "^1.0.0",
    "express-validator": "^6.3.1",
    "fb": "^2.0.0",
    "fs-extra": "^8.1.0",
    "google-auth-library": "^5.9.2",
    "http-errors": "^1.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.8.10",
    "mongoose-validator": "^2.1.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nanoid": "^2.1.11",
    "nodemailer": "^6.4.2",
    "nodemailer-mailgun-transport": "^2.0.0",
    "nodemon": "^2.0.2",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.17.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
